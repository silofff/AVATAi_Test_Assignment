<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="AVATAi.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:AVATAi.Views"
    Title="AVATAi">
    <TabBar>
        <ShellContent Title="Students" ContentTemplate="{DataTemplate views:MainPage}" />
        <ShellContent Title="Form" ContentTemplate="{DataTemplate views:FormPage}" />
        <ShellContent Title="Browser" ContentTemplate="{DataTemplate views:WebPage}" />
    </TabBar>
</Shell>


